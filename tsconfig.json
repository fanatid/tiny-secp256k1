{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationDir": "./types",
    "outDir": "./lib",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noPropertyAccessFromIndexSignature": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src_ts/*.ts"]
}
